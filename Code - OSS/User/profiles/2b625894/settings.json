{
    "workbench.colorTheme": "Ros√© Pine Moon",
    "gitlab.customQueries": [
        {
            "name": "Issues assigned to me",
            "type": "issues",
            "scope": "assigned_to_me",
            "state": "opened",
            "noItemText": "There is no issue assigned to you."
        },
        {
            "name": "Issues created by me",
            "type": "issues",
            "scope": "created_by_me",
            "state": "opened",
            "noItemText": "There is no issue created by you."
        },
        {
            "name": "Merge requests assigned to me",
            "type": "merge_requests",
            "scope": "assigned_to_me",
            "state": "opened",
            "noItemText": "There is no MR assigned to you."
        },
        {
            "name": "Merge requests I'm reviewing",
            "type": "merge_requests",
            "reviewer": "<current_user>",
            "state": "opened",
            "noItemText": "There is no MR for you to review."
        },
        {
            "name": "Merge requests created by me",
            "type": "merge_requests",
            "scope": "created_by_me",
            "state": "opened",
            "noItemText": "There is no MR created by you."
        },
        {
            "name": "All project merge requests",
            "type": "merge_requests",
            "scope": "all",
            "state": "opened",
            "noItemText": "The project has no merge requests"
        }
    ],
    "gitlab.ignoreCertificateErrors": true,
    "git.autofetch": true,
    "git.confirmSync": false,
    "git.enableSmartCommit": true,
    "explorer.confirmDragAndDrop": false,
    "git.openRepositoryInParentFolders": "always",
    "explorer.confirmDelete": false,
    "debug.onTaskErrors": "showErrors",
    "editor.suggest.preview": true,
    "editor.quickSuggestions": {
        "other": "inline"
    },
    "redhat.telemetry.enabled": false,
    "spellchecker.suggestionSeverity": "Hint",
    "latex-workshop.latex.autoClean.run": "onSucceeded",
    "security.workspace.trust.untrustedFiles": "open",
    "gitlens.ai.experimental.generateCommitMessage.enabled": false,
    "[json]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "workbench.startupEditor": "none",
    "spellchecker.documentTypes": [
        "markdown",
        "latex",
        "plaintext"
    ],
    "black-formatter.args": [
        "--verbose",
        "--config /config/black.toml"
    ],
    "pylint.args": [
        "--extension-pkg-allow-list=cv2",
        "--disable=E1101",
        "--disable=C0115",
        "--disable=C0114",
        "--disable=C0116"
    ],
    "pylint.importStrategy": "fromEnvironment",
    "flake8.args": [
        "--max-line-length=120"
    ],
    "editor.autoClosingBrackets": "always",
    "editor.autoClosingComments": "always",
    "workbench.iconTheme": "rose-pine-icons",
    "material-icon-theme.folders.theme": "specific",
    "material-icon-theme.saturation": 1,
    "[jsonc]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[yaml]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "autoDocstring.docstringFormat": "sphinx",
    "[xml]": {
        "editor.defaultFormatter": "fabianlauer.vs-code-xml-format"
    },
    "gitlab.cert": "",
    "[python]": {
        "editor.defaultFormatter": "ms-python.black-formatter"
    },
    "hediet.vscode-drawio.resizeImages": null,
    "C_Cpp_Runner.cppCompilerPath": "",
    "C_Cpp_Runner.cCompilerPath": "",
    "cmake.configureOnOpen": true,
    "hediet.vscode-drawio.theme": "atlas",
    "cmake.showOptionsMovedNotification": false,
    "markdown-pdf.plantumlCloseMarker": "```",
    "markdown-pdf.plantumlOpenMarker": "```plantuml",
    "cmake.pinnedCommands": [
        "workbench.action.tasks.configureTaskRunner",
        "workbench.action.tasks.runTask"
    ],
    "vsicons.dontShowNewVersionMessage": true,
    "update.mode": "manual",
    "C_Cpp.default.cppStandard": "c++23",
    "C_Cpp.default.cStandard": "c23",
    "C_Cpp_Runner.cppStandard": "c++23",
    "C_Cpp_Runner.cStandard": "c23",
    "markdown-preview-enhanced.plantumlServer": "https://kroki.io/plantuml/svg/",
    "liveshare.allowGuestDebugControl": true,
    "liveshare.allowGuestTaskControl": true,
    "liveshare.languages.allowGuestCommandControl": true,
    "liveshare.notebooks.allowGuestExecuteCells": true,
    "hexeditor.columnWidth": 16,
    "hexeditor.showDecodedText": true,
    "hexeditor.defaultEndianness": "little",
    "hexeditor.inspectorType": "aside",
    "[markdown]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "mypy-type-checker.args": [
        "--install-types"
    ],
    "diffEditor.ignoreTrimWhitespace": false,
    "lldb.suppressUpdateNotifications": true,
    "gitlab.duoCodeSuggestions.enabled": false,
    "gitlab.duoChat.enabled": false,
    "gitlab.duo.enabledWithoutGitlabProject": false,
    "gitlab.duoCodeSuggestions.openTabsContext": false,
    "gitlab.keybindingHints.enabled": false,
    "spellchecker.language": "en, de",
    "chat.commandCenter.enabled": false,
    "dev.containers.dockerComposePath": "podman-compose",
    "dev.containers.defaultExtensionsIfInstalledLocally": [
        "GitHub.vscode-pull-request-github"
    ],
    "dev.containers.dockerPath": "podman",
    "python.terminal.activateEnvInCurrentTerminal": true,
    "editor.inlineSuggest.edits.showCollapsed": true,
    "editor.inlineSuggest.enabled": false,
    "editor.inlayHints.enabled": "offUnlessPressed"
}